<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.automatic.telling.machine">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="ATM"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeScreen" />
        <activity android:name=".Operator_Login" />
        <activity android:name=".CashDetails" />
        <activity android:name=".UserLogin" />
        <activity android:name=".UserPinCheck" />
        <activity android:name=".transactions.Withdrawl" />
        <activity android:name=".transactions.Deposit" />
        <activity android:name=".transactions.Transfer" />
        <activity android:name=".transactions.Enquiry" />
        <activity android:name=".transactions.UpdatePassbook" />
        <activity android:name=".transactions.PinChange"></activity>
    </application>

</manifest>